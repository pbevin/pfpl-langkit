{
  "name": "PrimRec",
  "version": "0.0.1",
  "description": "Adventures with System T as described in Practical Foundations for Programming Languages, 2nd Ed.",
  "scripts": {
    "prestart": "npm run remove-dist",
    "start": "parallelshell \"npm run lint:tools\" \"npm run lint:styles\" \"npm run test:watch\" \"npm run open:src\"",
    "open:src": "babel-node tools/srcServer.js",
    "lint:fix": "eslint --fix src webpack.config.js tools",
    "lint:tools": "eslint webpack.config.js tools",
    "lint:styles": "postcss --watch --use stylelint --use postcss-reporter src/**/*.css -o /dev/null",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "npm run test && babel-node tools/build.js",
    "build:src": "babel-node tools/build.js",
    "test": "cross-env NODE_ENV=test mocha --reporter spec --compilers js:babel-core/register --require babel-polyfill --recursive \"./src/**/*.spec.js\"",
    "test:coverage": "istanbul cover _mocha -- -u exports --compilers js:babel-core/register --require babel-polyfill --recursive \"./src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "test:debug": "iron-node node_modules/mocha/bin/_mocha --compilers js:babel-core/register --require babel-polyfill --recursive \"./src/**/*.spec.js\""
  },
  "author": "Pete Bevin",
  "license": "GPL2",
  "dependencies": {
    "babel-polyfill": "6.6.1",
    "brace": "0.7.0",
    "jq-console": "2.13.2",
    "jquery": "2.2.0",
    "lodash": "4.3.0",
    "normalize.css": "3.0.3",
    "object-assign": "4.0.1",
    "parsimmon": "0.7.0",
    "react": "0.14.7",
    "react-ace": "3.1.0",
    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "redux": "3.3.1"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "babel-core": "6.7.0",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "browser-sync": "2.11.1",
    "chai": "3.4.1",
    "chalk": "1.1.1",
    "cheerio": "0.19.0",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "cssnext": "1.8.4",
    "cssnext-loader": "1.0.1",
    "eslint": "1.10.3",
    "eslint-loader": "1.2.0",
    "eslint-plugin-react": "3.15.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "imports-loader": "0.6.5",
    "istanbul": "1.0.0-alpha.2",
    "json-loader": "0.5.4",
    "mocha": "2.3.4",
    "nyc": "6.1.1",
    "parallelshell": "2.0.0",
    "postcss": "5.0.16",
    "postcss-cli": "2.5.1",
    "postcss-loader": "0.8.1",
    "postcss-reporter": "1.3.1",
    "precss": "1.4.0",
    "redbox-react": "1.2.0",
    "rimraf": "2.5.0",
    "style-loader": "0.13.0",
    "stylelint": "4.3.3",
    "stylelint-config-standard": "3.0.0",
    "stylelint-config-suitcss": "4.0.0",
    "stylelint-loader": "0.2.3",
    "watch": "0.17.1",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0",
    "yargs": "3.32.0"
  },
  "private": true
}
